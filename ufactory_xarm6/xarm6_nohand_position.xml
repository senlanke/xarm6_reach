<mujoco model="xarm6">
  <compiler angle="radian" meshdir="assets/"/>

  <option integrator="implicitfast"/>  <!-- 仿真选项：使用implicitfast积分器 -->

  <default>  <!-- 默认属性定义部分 -->
    <default class="xarm6">  <!-- 定义xarm7类的默认属性 -->
      <geom type="mesh" material="white"/>
    </default>
  </default>

  <asset>
    <mesh name="base" file="base.stl"/>
    <mesh name="base_vhacd" file="base_vhacd.obj"/>
    <mesh name="link1" file="link1.stl"/>
    <mesh name="link1_vhacd" file="link1_vhacd.obj"/>
    <mesh name="link2" file="link2.stl"/>
    <mesh name="link2_vhacd" file="link2_vhacd.obj"/>
    <mesh name="link3" file="link3.stl"/>
    <mesh name="link3_vhacd" file="link3_vhacd.obj"/>
    <mesh name="link4" file="link4.stl"/>
    <mesh name="link4_vhacd" file="link4_vhacd.obj"/>
    <mesh name="link5" file="link5.stl"/>
    <mesh name="link5_vhacd" file="link5_vhacd.obj"/>
    <mesh name="link6" file="link6.stl"/>
    <mesh name="link6_vhacd" file="link6_vhacd.obj"/>

    <material name="white" rgba="1 1 1 1"/>  <!-- 定义白色材质，RGBA值为1 1 1 1（不透明白） -->
    <material name="gray" rgba="0.753 0.753 0.753 1"/>  <!-- 定义灰色材质，RGBA值为0.753 0.753 0.753 1 ，第四个值为透明度-->
  </asset>

  <worldbody>
    <body name="link_base" pos="0 0 0">
      <geom name="link_base_visual" type="mesh"  group="1" density="0" mesh="base"/>
      <geom name="link_base_collision" type="mesh"  mesh="base_vhacd"/>
      <body name="link1" pos="0 0 0.267">
        <inertial pos="-0.002 0.02692 -0.01332" quat="0.983139 0.180751 -0.00455228 0.0272976" mass="2.16" diaginertia="0.00539487 0.00519906 0.00281397"/>
        <joint name="joint1" pos="0 0 0" axis="0 0 1" range="-6.28319 6.28319" damping="1" frictionloss="1" limited="true"/>
        <geom name="link1_visual" type="mesh"  group="1" density="0"  mesh="link1"/>
        <geom name="link1_collision" type="mesh" mesh="link1_vhacd"/>
        <body name="link2" quat="0.707105 -0.707108 0 0">
          <inertial pos="0.03531 -0.21398 0.03386" quat="0.71118 0.614686 0.139936 -0.311132" mass="1.71" diaginertia="0.017867 0.017867 0.017867"/>
          <joint name="joint2" pos="0 0 0" axis="0 0 1" range="-2.059 2.0944" damping="1" frictionloss="1" limited="true"/>
          <geom name="link2_visual" type="mesh"  group="1" density="0" mesh="link2"/>
          <geom name="link2_collision" type="mesh" mesh="link2_vhacd"/>
          <body name="link3" pos="0.0535 -0.2845 0">
            <inertial pos="0.06781 0.10749 0.01457" quat="0.119516 0.409082 -0.734824 0.527638" mass="1.384" diaginertia="0.004543 0.004543 0.004543"/>
            <joint name="joint3" pos="0 0 0" axis="0 0 1" range="-3.927 0.19198" damping="1" frictionloss="1" limited="true"/>
            <geom name="link3_visual" type="mesh"  group="1" density="0" mesh="link3"/>
            <geom name="link3_collision" type="mesh" mesh="link3_vhacd"/>
            <body name="link4" pos="0.0775 0.3425 0" quat="0.707105 -0.707108 0 0">
              <inertial pos="-0.00021 0.02578 -0.02538" quat="0.994019 0.0752563 -0.00686804 0.0788347" mass="1.115" diaginertia="0.00440091 0.00406855 0.00103407"/>
              <joint name="joint4" pos="0 0 0" axis="0 0 1" range="-6.28319 6.28319" damping="1" frictionloss="1" limited="true"/>
              <geom name="link4_visual" type="mesh"  group="1" density="0" mesh="link4"/>
              <geom name="link4_collision" type="mesh" mesh="link4_vhacd"/>
              <body name="link5" quat="0.707105 0.707108 0 0">
                <inertial pos="0.05428 0.01781 0.00543" quat="-0.0766302 0.578118 -0.269638 0.766291" mass="1.275" diaginertia="0.00289757 0.0023276 0.000951789"/>
                <joint name="joint5" pos="0 0 0" axis="0 0 1" range="-1.69297 3.14159"  damping="1" frictionloss="1" limited="true"/>
                <geom name="link5_visual" type="mesh"  group="1" density="0" mesh="link5"/>
                <geom name="link5_collision" type="mesh" mesh="link5_vhacd"/>
                <body name="link6" pos="0.076 0.097 0" quat="0.707105 -0.707108 0 0">
                  <inertial pos="0 0.00064 -0.00952" quat="0 0.707107 0 0.707107" mass="0.1096" diaginertia="7.9715e-05 4.8111e-05 4.5293e-05"/>
                  <joint name="joint6" pos="0 0 0" axis="0 0 1" range="-6.28319 6.28319" damping="1" frictionloss="1" limited="true"/>
                  <geom name="link6_visual" type="mesh"  group="1" density="0" material="gray" mesh="link6"/>
                  <geom name="link6_collision" type="mesh" material="gray" mesh="link6_vhacd"/>
                  <site name="ee_site" pos="0 0 0" material="gray"/>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator> 
    <position name="joint1_position" joint="joint1" kp="1000" kv="200" ctrlrange="-6.28319 6.28319" ctrllimited="true"/>
    <position name="joint2_position" joint="joint2" kp="1000" kv="200" ctrlrange="-2.059 2.0944" ctrllimited="true"/>
    <position name="joint3_position" joint="joint3" kp="1000" kv="200" ctrlrange="-3.927 0.19198" ctrllimited="true"/>
    <position name="joint4_position" joint="joint4" kp="1000" kv="200" ctrlrange="-6.28319 6.28319" ctrllimited="true"/>
    <position name="joint5_position" joint="joint5" kp="1000" kv="200" ctrlrange="-1.69297 3.14159" ctrllimited="true"/>
    <position name="joint6_position" joint="joint6" kp="1000" kv="200" ctrlrange="-6.28319 6.28319" ctrllimited="true"/>
  </actuator>

  <contact>
    <exclude body1="link_base" body2="link1" />
    <exclude body1="link1" body2="link2" />
    <exclude body1="link2" body2="link3" />
    <exclude body1="link3" body2="link4" />
    <exclude body1="link4" body2="link5" />
    <exclude body1="link5" body2="link6" />
  </contact>

  <!-- <keyframe>
    <key name="home" qpos="0 -90 -30 0 30 0 "/>
  </keyframe> -->

</mujoco>
